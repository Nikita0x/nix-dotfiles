{
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"editor.fontSize": 15,
	"editor.fontFamily": "Monospace",
	"editor.fontLigatures": true,
	"editor.linkedEditing": true,
	"editor.insertSpaces": true,
	"editor.tabSize": 4,
	"explorer.confirmDragAndDrop": false,
	"emmet.includeLanguages": {
		"vue": "html",
		"vue-html": "html",
		"javascript": "javascriptreact",
		"django-html": "html"
	},
	"workbench.list.smoothScrolling": true,
	"terminal.integrated.smoothScrolling": true,
	"editor.smoothScrolling": false,
	"editor.scrollBeyondLastLine": false,
	"editor.semanticHighlighting.enabled": true,
	"editor.links": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.cursorStyle": "line-thin",
	"editor.cursorWidth": 3,
	"editor.cursorBlinking": "phase",
	"editor.bracketPairColorization.enabled": false,
	"workbench.colorCustomizations": {
		"tab.activeBackground": "#00042b90",
		"editorCursor.foreground": "#fff",
		"editor.background": "#1e1e1e",
		"editor.selectionBackground": "#000",
		"editorLineNumber.foreground": "#676867",
		"editorLineNumber.activeForeground": "#fff"
		// "editorBracketHighlight.foreground1": "red",
		// "editorBracketHighlight.foreground2": "red",
		// "editorBracketHighlight.foreground3": "red",
		// "editorBracketHighlight.foreground4": "red",
		// "editorBracketHighlight.foreground5": "red",
		// "editorBracketHighlight.foreground6": "red"
	},
	"editor.formatOnSave": true,
	"explorer.confirmPasteNative": false,
	"workbench.sideBar.location": "right",
	"editor.stickyScroll.enabled": false,
	"breadcrumbs.enabled": false,
	"editor.minimap.enabled": false,

	"security.workspace.trust.untrustedFiles": "open",
	"liveServer.settings.host": "localhost",
	"workbench.iconTheme": "icons",
	"liveServer.settings.donotVerifyTags": true,
	"editor.detectIndentation": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.wordWrap": "bounded",
	"emmet.preferences": {},
	"errorLens.enabledDiagnosticLevels": [
		"error",
		"warning"
		// "info"
	],
	"git.openRepositoryInParentFolders": "never",
	"cSpell.languageSettings": [],
	"workbench.startupEditor": "none",
	"editor.fontVariations": false,
	"terminal.integrated.env.windows": {},
	"workbench.settings.useSplitJSON": true,
	"json.schemas": [],
	"explorer.openEditors.visible": 1,
	"cSpell.allowedSchemas": [
		"file",
		"gist",
		"sftp",
		"untitled",
		"vscode-notebook-cell",
		"vscode-scm",
		"vscode-userdata",
		"html"
	],
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"[jsonc]": {},
	"javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
	"githubPullRequests.createOnPublishBranch": "never",
	"git.openAfterClone": "always",
	"liveServer.settings.donotShowInfoMsg": true,
	"ecsstractor_port.bem_nesting": true,
	"ecsstractor_port.attributes": "className",
	"javascript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"editor.inlayHints.padding": true,
	"workbench.colorTheme": "Default Dark+",
	"prettier.tabWidth": 4,
	"prettier.singleAttributePerLine": true,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"prettier.vueIndentScriptAndStyle": true,

	"editor.codeLensFontFamily": "PT Sans Narrow",
	"editor.multiCursorModifier": "ctrlCmd",
	"terminal.integrated.fontFamily": "monospace",
	"editor.cursorSurroundingLines": 8,
	"notebook.confirmDeleteRunningCell": false,
	"explorer.confirmDelete": false,
	"explorer.confirmUndo": "verbose",
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	"editor.guides.bracketPairs": "active",
	"editor.hideCursorInOverviewRuler": true,
	"editor.inlayHints.enabled": "off",

	"terminal.integrated.env.linux": {},
	"git.ignoreRebaseWarning": true,
	//Custom syntax highlighter
	"editor.tokenColorCustomizations": {
		"functions": {
			"fontStyle": "underline",
			"foreground": "#d97706"
		},
		"numbers": {
			"foreground": "#408080"
		},
		"keywords": {
			"foreground": "#9872A2",
			"fontStyle": "italic"
		},
		"strings": {
			"foreground": "#9AA83A",
			"fontStyle": ""
		},
		"comments": "#525252",
		"types": "#931600",
		// "variables": "#4FC1FF",

		// Javascript / Typescript
		"textMateRules": [
			// colors for true / false
			{
				"scope": [
					"constant.language.boolean.false",
					"constant.language.boolean.true",
					"constant.language.json.comments",
					"constant.language.null",
					"constant.language.undefined"
				],
				"settings": {
					"foreground": "#408080"
				}
			},
			// const
			{
				"scope": ["variable.other.constant"],
				"settings": {
					"foreground": "#3b8cb8"
				}
			},
			// this keyword, spread operator
			{
				"scope": [
					"variable.language.this",
					"keyword.operator.spread",
					"keyword.operator.rest"
				],
				"settings": {
					"foreground": "#C7444A"
				}
			},
			// arguments/parameters to a function
			{
				"scope": ["variable.parameter"],
				"settings": {
					"foreground": "#078448be",
					"fontStyle": "italic"
				}
			},
			// enum
			{
				"scope": ["entity.name.type.enum"],
				"settings": {
					"foreground": "#ffc85a"
				}
			},
			// enum member
			{
				"scope": ["variable.other.enummember"],
				"settings": {
					"foreground": "#9CDCFE"
				}
			},
			// IN PROGRESS: console, Math,
			// {
			// 	"scope": ["support.variable"],
			// 	"settings": {
			// 		"foreground": "#0d9c00"
			// 	}
			// },
			// IN PROGRESS, interface,
			{
				"scope": ["entity.name.type.interface"],
				"settings": {
					"foreground": "#53b20b"
				}
			},
			// other
			{
				"scope": [
					"punctuation.accessor",
					"punctuation.terminator.statement",
					"punctuation.definition.parameters.begin",
					"punctuation.definition.parameters.end",
					"punctuation.definition.typeparameters.begin",
					"punctuation.definition.typeparameters.end",
					"meta.brace.round",
					"meta.type.annotation",
					"punctuation.definition.block",
					"keyword.operator.type.annotation",
					"punctuation.separator.comma",
					"punctuation.separator.parameter",
					"keyword.operator.assignment",
					"meta.brace.square",
					"punctuation.definition.interpolation.begin.html.vue",
					"punctuation.definition.interpolation.end.html.vue",
					"punctuation.definition.binding-pattern.object.js",
					"punctuation.separator.key-value",
					"punctuation.terminator.rule",
					"punctuation.section.keyframes.begin.bracket.curly",
					"punctuation.section.keyframes.end.bracket.curly",
					"meta.property-value",
					"punctuation.section.property-list.begin.bracket.curly",
					"punctuation.section.property-list.end.bracket.curly",
					"punctuation.definition.binding-pattern",
					"keyword.operator.type.ts"
				],
				"settings": {
					"foreground": "#676867"
				}
			},
			//logical operators: ||, &&, ??
			{
				"scope": ["keyword.operator.logical"],
				"settings": {
					"foreground": "#676867"
				}
			},
			// ternary operators: ?, :
			{
				"scope": ["keyword.operator.ternary"],
				"settings": {
					"foreground": "#676867"
				}
			},
			// relational operators: >, <
			{
				"scope": ["keyword.operator.relational"],
				"settings": {
					"foreground": "#676867"
				}
			},
			// comparison operators: !==, !=,  ==, ===
			{
				"scope": ["keyword.operator.comparison"],
				"settings": {
					"foreground": "#676867"
				}
			},
			// html tags < and >, />
			{
				"scope": [
					"punctuation.definition.tag.begin.html",
					"punctuation.definition.tag.end.html"
				],
				"settings": {
					"foreground": "#676867"
				}
			},
			// new operator, typeof operator
			{
				"scope": ["keyword.operator.new"],
				"settings": {
					// "foreground": "#676867",
					"foreground": "#9872A2"
				}
			},
			// highlight key fields in object literals
			// {
			// 	// "scope": [
			// 	// 	"variable.other.property",
			// 	// 	"variable.other.property.js",
			// 	// 	"variable.other.object.property.js"
			// 	// ],
			// 	// "settings": {
			// 	// 	"foreground": "#ff0015",
			// 	// 	"fontStyle": "underline strikethrough"
			// 	// }
			// },

			// types
			{
				"scope": ["support.type"],
				"settings": {
					// "foreground": "#676867"
					"foreground": "#4EC9B0"
				}
			},
			// escape character
			{
				"scope": "constant.character.escape",
				"settings": {
					"foreground": "#8080FF"
				}
			},
			// keyword "in", dynamic import
			{
				"scope": [
					"keyword.operator.expression.in",
					"keyword.operator.expression.import",
					"keyword.operator.expression.typeof",
					"keyword.operator.expression.keyof.ts"
				],
				"settings": {
					"foreground": "#9872A2"
				}
			},
			// extends keyword
			{
				"scope": "storage.modifier",
				"settings": {
					"foreground": "#9872A2"
				}
			},
			// async keyword
			{
				"scope": "storage.modifier.async",
				"settings": {
					"foreground": "#9872A2"
				}
			},
			// optional ? in func signature
			{
				"scope": "keyword.operator.optional",
				"settings": {
					"foreground": "#676867"
					// "fontStyle": "strikethrough",
				}
			},
			// arrow function
			{
				"scope": "storage.type.function.arrow",
				"settings": {
					"foreground": "#d97706",
					"fontStyle": "underline"
				}
			},
			// arithmetic operators
			{
				"scope": [
					"keyword.operator.arithmetic",
					"keyword.operator.increment",
					"keyword.operator.decrement"
				],
				"settings": {
					"foreground": "#F92672"
				}
			},
			// html attributes color
			{
				"scope": [
					"entity.other.attribute-name.html.vue",
					"entity.other.attribute-name.html",
					"punctuation.separator.key-value.html",
					"punctuation.definition.string.begin.html.vue",
					"punctuation.definition.string.end.html.vue",
					"punctuation.attribute-shorthand.event.html.vue",
					"punctuation.attribute-shorthand.bind.html.vue",
					"meta.attribute.directive.vue"
				],
				"settings": {
					"foreground": "#D0B344"
				}
			},
			// json settings
			{
				"scope": [
					"punctuation.definition.dictionary.begin.json.comments",
					"punctuation.definition.dictionary.end.json.comments",
					"punctuation.definition.array.begin.json.comments",
					"punctuation.definition.array.end.json.comments",
					"punctuation.separator.dictionary.key-value.json.comments"
				],
				"settings": {
					"foreground": "#676867"
				}
			},
			// CSS settings
			{
				"scope": [
					"entity.other.attribute-name.class.css",
					"meta.property-list.css",
					"entity.name.tag.css"
				],
				"settings": {
					"foreground": "#D0B344"
				}
			},
			{
				"scope": "support.type.property-name.css",
				"settings": {
					"foreground": "#676867"
				}
			},
			{
				"scope": [
					"punctuation.section.property-list.begin.bracket.curly.css",
					"punctuation.section.property-list.end.bracket.curly.css",
					"punctuation.section.property-list.end.bracket.curly.css",
					"punctuation.section.function.begin.bracket.round.css",
					"punctuation.section.function.end.bracket.round.css",
					"keyword.other.unit",
					"punctuation.section.keyframes.begin.scss",
					"punctuation.section.keyframes.end.scss",
					// "source.css.scss",
					"keyword.operator.css",
					"punctuation.section.function.scss",
					"punctuation.separator.delimiter.scss"
				],
				"settings": {
					"foreground": "#676867"
				}
			},
			{
				"scope": ["support.constant.property-value.css"],
				"settings": {
					"foreground": "#C7444A"
				}
			},
			{
				"scope": ["constant.other.color.rgb-value.hex.css"],
				"settings": {
					"foreground": "#8080FF"
				}
			},
			{
				"scope": ["meta.selector.css"],
				"settings": {
					"foreground": "#D7BA7D"
				}
			},
			// SCSS
			{
				"scope": ["keyword.other.important.scss"],
				"settings": {
					"foreground": "#9872A2"
				}
			},
			// C langauge
			{
				"scope": [
					"punctuation.section.arguments.begin.bracket.round.c",
					"punctuation.section.arguments.end.bracket.round.c",
					"punctuation.section.parens.begin.bracket.round.c",
					"punctuation.section.parens.end.bracket.round.c",
					"punctuation.section.block.begin.bracket.curly.c",
					"punctuation.section.block.end.bracket.curly.c",
					"punctuation.separator.delimiter.c",
					"punctuation.section.parameters.begin.bracket.round.c",
					"punctuation.section.parameters.end.bracket.round.c",
					"keyword.operator.c"
				],
				"settings": {
					"foreground": "#676867"
				}
			},
			// Dart Langauge

			{
				"scope": [
					"punctuation.comma.dart",
					"punctuation.terminator.dart",
					"punctuation.dot.dart",
					"keyword.operator.ternary.dart",
					"keyword.operator.bitwise.dart"
				],
				"settings": {
					"foreground": "#676867"
				}
			},
			{
				"scope": ["keyword.operator.closure.dart"],
				"settings": {
					"fontStyle": "underline",
					"foreground": "#d97706"
				}
			},
			{
				"scope": ["storage.type.annotation.dart"],
				"settings": {
					"foreground": "#23c000",
					"fontStyle": ""
				}
			}
		]
	},
	"editor.semanticTokenColorCustomizations": {
		"enabled": true,
		"rules": {
			"keyword": {
